    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;SELECT V_IND_VALUE.REP_LABEL,V_IND_VALUE.AN_LABEL,V_IND_VALUE.IND_LABEL,  CASE WHEN IND_VALUE.INDV_VALUE_TYPE_INDICATOR='I' THEN (CASE WHEN IND_DEN.IND_UUID='_ccIR4BF2Ed2PKb6nEJEvhw' THEN (CASE WHEN IND_VALUE.INDV_ROW_COUNT=0 THEN 0.0 ELSE IND_VALUE.INDV_INT_VALUE/IND_VALUE.INDV_ROW_COUNT END)  ELSE IND_VALUE.INDV_INT_VALUE  END ) ELSE (CASE WHEN INDV_VALUE_TYPE_INDICATOR='R' THEN     (CASE WHEN IND_DEN.IND_UUID='_ccI48RF2Ed2PKb6nEJEvhw' THEN (CASE WHEN IND_VALUE.INDV_ROW_COUNT=0 THEN 0.0 ELSE IND_VALUE.INDV_REAL_VALUE/IND_VALUE.INDV_ROW_COUNT END)         ELSE IND_VALUE.INDV_REAL_VALUE END)  ELSE 0  END)  END AS INDV_INT_VALUE, case when V_IND_VALUE.ELT_CATALOG_NAME like 'N/A (TDQ)' then 'N/A' else V_IND_VALUE.ELT_CATALOG_NAME end as ELT_CATALOG_NAME,case when V_IND_VALUE.ELT_SCHEMA_NAME like 'N/A (TDQ)' then 'N/A' else V_IND_VALUE.ELT_SCHEMA_NAME end as ELT_SCHEMA_NAME,case when V_IND_VALUE.ELT_TABLE_NAME like 'N/A (TDQ)' then 'N/A' else V_IND_VALUE.ELT_TABLE_NAME end as ELT_TABLE_NAME,case when V_IND_VALUE.ELT_COLUMN_NAME like 'N/A (TDQ)' then 'N/A' else V_IND_VALUE.ELT_COLUMN_NAME end as ELT_COLUMN_NAME,case when V_IND_VALUE.INO_REGEXP like 'NULL%' then 'N/A' else V_IND_VALUE.INO_REGEXP end AS INO_REGEXP,IND_VALUE.AN_RUNTIME,IND_VALUE.INDV_DTHRESH_OK,IND_VALUE.INDV_ITHRESH_OK,IND_VALUE.INDV_ITHRESH_PERC_OK,IF(IND_VALUE.INDV_ITHRESH_LOW is NULL,'N/A',IND_VALUE.INDV_ITHRESH_LOW) AS INDV_ITHRESH_LOW,IF(IND_VALUE.INDV_ITHRESH_HI is NULL,'N/A',IND_VALUE.INDV_ITHRESH_HI) AS INDV_ITHRESH_HI,IF(IND_VALUE.INDV_ITHRESH_PERC_LOW is NULL,'N/A',concat(IND_VALUE.INDV_ITHRESH_PERC_LOW*100,'%')) AS INDV_ITHRESH_PERC_LOW,IF(IND_VALUE.INDV_ITHRESH_PERC_HI is NULL,'N/A',concat(IND_VALUE.INDV_ITHRESH_PERC_HI*100,'%')) AS INDV_ITHRESH_PERC_HI FROM TDQ_V_IND_HISTO V_IND_VALUE, TDQ_INDICATOR_VALUE IND_VALUE, TDQ_INDICATOR_DEFINITION IND_DEN,TDQ_ANALYZED_ELEMENT ANA_ELE, TDQ_INDICATOR_OPTIONS OPT WHERE V_IND_VALUE.RUN_TIME=IND_VALUE.AN_RUNTIME AND V_IND_VALUE.INO_REGEXP=OPT.INO_REGEXP AND IND_VALUE.OPTION_PK=OPT.INO_PK AND V_IND_VALUE.IND_UUID= IND_DEN.IND_UUID AND IND_DEN.IND_PK=IND_VALUE.INDICATOR_PK  AND ANA_ELE.ELT_PK=IND_VALUE.ELEMENT_PK AND V_IND_VALUE.ELT_UUID=ANA_ELE.ELT_UUID AND (IND_VALUE.INDV_DTHRESH_OK='N' OR IND_VALUE.INDV_ITHRESH_OK='N' OR IND_VALUE.INDV_ITHRESH_PERC_OK='N') AND (V_IND_VALUE.REP_UUID='_cMsQoNRvEfC3172WwR85Fw') AND IND_VALUE.REP_RUNTIME in (select max(rep_runtime) from TDQ_INDICATOR_VALUE join TDQ_ANALYSIS ON TDQ_ANALYSIS.AN_PK=TDQ_INDICATOR_VALUE.ANALYSIS_PK AND TDQ_ANALYSIS.REP_UUID='_cMsQoNRvEfC3172WwR85Fw')&quot;"/>
